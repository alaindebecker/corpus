    <jsScripts>
      <jsScript>
        <jsScript_type>0</jsScript_type>
        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>/*******************

  Jacques  Savoy, 1999

  For words of five or more letters
    if the final letter is «-x» 
      then if final is «-aux» 
        then replace final «-aux» by «-al»
        else remove final «-x»

    else (words not ending with «-x»)
      if final letter is «-s» 
        then remove final «-s»
          if final letter is «-r» 
            then remove final «-r»
          if final letter is «-e» 
            then remove final «-e»
          if final letter is «-é» 
            then remove final «-é» 
          if final two letters are the same
            then remove final letter
  else 
    does not alter words of four or less letters

*******************/

var stem = word;

if(word.length>4){

	stem = word
               .replace(/aux$/, 'al')
               .replace(/x$/, '')
               .replace(/s$/, '')
               .replace(/r$/, '')
               .replace(/e$/, '')
               .replace(/é$/, '')
               .replace(/([a-z])\1+$/, '$1')

}</jsScript_script>
      </jsScript>
    </jsScripts>
    <fields>
      <field>
        <name>stem</name>
        <rename>stem</rename>
        <type>String</type>
        <length>-1</length>
        <precision>-1</precision>
        <replace>N</replace>
      </field>
