    <jsScripts>
      <jsScript>
        <jsScript_type>0</jsScript_type>
        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>/*******************

  Jacques  Savoy, 1999
  http://members.unine.ch/jacques.savoy/clef/frenchStemmerPlus.txt

*******************/

var stem = word;

stem = word
           .replace(/([^e])aux$/, '$1al')
           .replace(/aux$/, 'au')
           .replace(/x$/, '')
           .replace(/s$/, '')

           .replace(/issement$/, 'ir')
           .replace(/issant$/, 'ir')
           .replace(/ivement$/, 'if')
           .replace(/ement$/, 'e')
           .replace(/ficatrice$/, 'fier')
           .replace(/catrice$/, 'quer')
           .replace(/cateur$/, 'quer')
           .replace(/atrice$/, 'er')
           .replace(/ateur$/, 'er')
           .replace(/trice$/, 'teur')
           .replace(/ième$/, '')
           .replace(/teuse$/, 'ter')
           .replace(/teur$/, 'ter')
           .replace(/euse$/, 'eu-')
//           .replace(/ère$/, 'er')
//           .replace(/ive$/, 'if')


</jsScript_script>
      </jsScript>
    </jsScripts>
    <fields>
