        <jsScript_script>/*******************

  Jacques  Savoy, 1999
  http://members.unine.ch/jacques.savoy/clef/frenchStemmerPlus.txt

  erreur :rule	word	stem
1	traveaux	traveau
2	aulx	aul
3	affreux	affreu
3	yeux	yeu
14	matrice	mer
14	productrice	producter
18	plieuse	plieu-
18	poreuse	poreu-
19	abrasive	abrasif
19	convive	convif

hauteur --> hauter

*******************/

var stem = word;

stem = stem
		   .replace(/([^e])aux$/, '$1al-')  //  1
           .replace(/aux$/,       'au-')    //  2
           .replace(/x$/,         '-')      //  3
           .replace(/s$/,         '-')      //  4

           .replace(/issement$/,  'ir-')    //  5
           .replace(/issant$/,   'ir-')     //  6
           .replace(/ivement$/,   'if-')    //  7
           .replace(/ement$/,     'e-')     //  8
           .replace(/ficatrice$/, 'fier-')  //  9
           .replace(/catrice$/,   'quer-')  // 10
           .replace(/cateur$/,    'quer-')  // 11
           .replace(/atrice$/,    'er-')    // 12
           .replace(/ateur$/,     'er-')    // 13
           .replace(/trice$/,     'teur-')  // 14
           .replace(/ième$/,      '-')      // 15
           .replace(/teuse$/,     'ter-')   // 16
           .replace(/teur$/,      'ter-')   // 17
           .replace(/euse$/,      'eu-')    // 18
           .replace(/ère$/,       'er-')    // 19
           .replace(/ive$/,       'if-')    // 20
           .replace(/olle$/,      'ou-')    // 21
           .replace(/nelle$/,     'n-')     // 22
           .replace(/nel$/,       'n-')     // 23
           .replace(/ète$/,       'et-')    // 24
           .replace(/ique$/,      '-')      // 25
           .replace(/esse$/,      'e-')     // 26
           .replace(/inage$/,     'in-')    // 27
           .replace(/ualisation$/,'uel-')   // 28
           .replace(/isation$/,   '-')      // 28
           .replace(/isateur$/,   '-')      // 28
           .replace(/ation$/,     '-')      // 29
           .replace(/ition$/,     '-')      // 30

           .replace(/-$/, '')

/*
           .replace(/ie$/, '')
           .replace(/r$/, '')
           .replace(/e$/, '')
           .replace(/é$/, '') 
*/

var len = word.length;</jsScript_script>
      </jsScript>
